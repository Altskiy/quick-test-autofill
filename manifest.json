{
    "name": "Quick Test Autofill",
    "description": "Генератор тестовых данных для реквизитов: мгновенно заполняйте ИНН, ОГРН, КПП в формах.",
    "homepage_url": "https://github.com/username/quick-test-autofill",
    "author": "Altskiy",
    "license": "MIT",
    "version": "1.0.5",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "generators/index.js",
                "data/dicts.js",
                "data/placeholders.js",
                "generators/birthdate.js",
                "generators/futuredate.js",
                "generators/innfl.js",
                "generators/innul.js",
                "generators/snils.js",
                "generators/ogrn.js",
                "generators/ogrnip.js",
                "generators/kpp.js",
                "helpers.js",
                "main.js"
            ],
            "css": [
                "styles.css"
            ],
            "run_at": "document_idle",
            "all_frames": true
        }
    ],
    "options_page": "options.html",
    "action": {
        "default_icon": {
            "16": "icons/icon_on_16.png",
            "32": "icons/icon_on_32.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon_on_16.png",
        "32": "icons/icon_on_32.png",
        "48": "icons/icon_on_48.png",
        "128": "icons/icon_on_128.png"
    }
}